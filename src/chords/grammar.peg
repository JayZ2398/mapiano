start := chord
tone := 'C' | 'D' | 'E' | 'F' | 'G' | 'A' | 'B'
note := tone=tone modifier={'#' | 'b'}?
root := note
major := major={'maj' | 'M'}
major2 := major=''
minor := minor={'min' | 'm'}
diminished := diminished={'o' | 'dim'}
augmented := augmented={'\+' | 'aug'}
quality := diminished | augmented | major | minor | major2
chord := root=root quality=quality $